<div class="container">
  <h1> <%= @url.address %></h1>
  <table class="table">
    <tr>
      <td> Name </td>
      <td> Data </td>
    </tr>
    <tr>
      <td> Average Response Time </td>
      <td> <%= @url.average_response_for_url %> milliseconds </td>
    </tr>
    <tr>
      <td> Minimum Response Time </td>
      <td> <%= @url.min_response_for_url %> milliseconds </td>
    </tr>
    <tr>
      <td> Maximum Response Time </td>
      <td> <%= @url.max_response_for_url %> milliseconds </td>
    </tr>
    <tr>
      <td> Response Times </td>
      <td> <%= @url.response_times_order %> </td>
    </tr>
    <tr>
      <td> Popular Referrers </td>
      <td> <%= @url.top_three_referrers %> </td>
    </tr>
    <tr>
      <td> Top User Agents </td>
      <td> <%= @url.popular_agents %> </td>
    </tr>
  </table>
</div>
